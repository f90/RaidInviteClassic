				_G[self:GetName().."BG"]:SetVertexColor(1, 1, 1, 1)
			</OnEnter>
			<OnLeave>
				_G[self:GetName().."BG"]:SetVertexColor(0.7, 0.7, 0.7, 0.7)
			</OnLeave>
			<OnLoad>
				self:GetScript("OnLeave")(self)
			</OnLoad>
		</Scripts>
