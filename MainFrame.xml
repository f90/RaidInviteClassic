	<Button name="RIC_MinimapButton" parent="Minimap" enableMouse="true" movable="true" hidden="false">
		<Size>
			<AbsDimension x="33" y="33"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER"/>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="RIC_MinimapButton_Icon" file="Interface\AddOns\RaidInviteClassic\img\minimap">
					<Size>
						<AbsDimension x="21" y="21"/>
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="7" y="-6"/>
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<Texture file="Interface\Minimap\MiniMap-TrackingBorder">
					<Size>
						<AbsDimension x="56" y="56"/>
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Frame name="RIC_MinimapButton_DraggingFrame" hidden="true">
				<Scripts>
					<OnUpdate>
						RIC_MinimapButton_DraggingFrame_OnUpdate()
					</OnUpdate>
				</Scripts>
			</Frame>
		</Frames>
		<HighlightTexture alphaMode="ADD" file="Interface\Minimap\UI-Minimap-ZoomButton-Highlight"/>
		<Scripts>
			<OnLoad>
				self:SetFrameStrata("MEDIUM") -- Minimap icon should be above minimap
				self:SetFrameLevel(8) -- Minimap icon should be above minimap
				self:RegisterForClicks("LeftButtonUp","RightButtonUp")
				self:RegisterForDrag("LeftButton","RightButton")
			</OnLoad>
			<OnDragStart>
				self:LockHighlight()
				RIC_MinimapButton_DraggingFrame:Show()
			</OnDragStart>
			<OnDragStop>
				self:UnlockHighlight()
				RIC_MinimapButton_DraggingFrame:Hide()
			</OnDragStop>
			<OnClick>
				RIC_MinimapButton_OnClick(button) -- do your thing in here, arg1 is mouse button clicked
			</OnClick>
			<OnEnter>
				RIC_MinimapButton_OnEnter(self)
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide()
			</OnLeave>
		</Scripts>
	</Button>
